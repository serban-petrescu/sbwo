{
	"_version": "1.1.0",
	"sap.app": {
		"_version": "1.1.0",
		"id": "spet.sbwo.web",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.32.0"
		},
		"dataSources": {
			"main": {
				"uri": "/private/api/odata/",
				"type": "OData",
				"settings": {
					"annotations": [],
					"odataVersion": "2.0",
					"localUri": ""
				}
			}
		}
	},
	"sap.ui": {
		"_version": "1.1.0",
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": ["sap_hcb", "sap_bluecrystal"]
	},
	"sap.ui5": {
		"_version": "1.1.0",
		"rootView": {
			"viewName": "spet.sbwo.web.view.Root",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "spet.sbwo.web.i18n.i18n"
				}
			},
			"": {
				"type": "sap.ui.model.odata.v2.ODataModel",
                "dataSource": "main",
                "settings": {
                    "metadataUrlParams": {
                        "sap-documentation": "heading"
                    },
                    "headers": {
                    	"X-Requested-With": "XMLHttpRequest"
                    }
                }
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "spet.sbwo.web.view",
				"controlId": "appMain",
				"controlAggregation": "pages",
				"transition": "slide",
				"async": true,
				"bypassed": {
					"target": ["home"]
				}
			},
			"routes": [{
				"pattern": "home",
				"name": "home",
				"target": "home"
			}, {
				"pattern": "person/{id}",
				"name": "person",
				"target": "person"
			}, {
				"pattern": "person-list/:search:",
				"name": "person-list",
				"target": "person-list"
			}, {
				"pattern": "person-create",
				"name": "person-create",
				"target": "person-create"
			}, {
				"pattern": "customizing",
				"name": "customizing",
				"target": "customizing"
			}, {
				"pattern": "deleted-list/:search:",
				"name": "deleted-list",
				"target": "deleted-list"
			}, {
				"pattern": "import",
				"name": "import",
				"target": "import"
			},{
				"pattern": "server-settings",
				"name": "server-settings",
				"target": "server-settings"
			},{
				"pattern": "user-settings",
				"name": "user-settings",
				"target": "user-settings"
			},{
				"pattern": "help/:id:",
				"name": "help",
				"target": "help"
			}, {
				"pattern": "expertise-create",
				"name": "expertise-create",
				"target": "expertise-create"
			}, {
				"pattern": "expertise-list/:search:",
				"name": "expertise-list",
				"target": "expertise-list"
			}, {
				"pattern": "expertise/{id}",
				"name": "expertise",
				"target": "expertise"
			}],
			"targets": {
				"home": {
					"viewName": "Home",
					"title": "{i18n>pagHomeTitle}",
					"viewId": "home"
				},
				"person": {
					"viewName": "entity.Person",
					"title": {
						"parts": [
							"view>/loaded", "i18n>pagEntityLoadingTitle", "i18n>pagEntityPersonPatternTitle", 
							"data>/type", "data>/firstName", "data>/lastName", "data>/name"
						],
						"formatter": "spet.sbwo.web.model.formatter.personPageTitle"
					},
					"viewId": "person"
				},
				"not-found": {
					"viewName": "entity.NotFound",
					"title": "{i18n>pagEntityNotFoundTitle}",
					"viewId": "not-found"
				},
				"person-create": {
					"viewName": "wizard.PersonCreate",
					"title": "{i18n>pagPersonCreateTitle}",
					"viewId": "person-create"
				},
				"person-list": {
					"viewName": "list.PersonList",
					"title": "{i18n>pagPersonListTitle}",
					"viewId": "person-list"
				},
				"deleted-list": {
					"viewName": "list.DeletedEntityList",
					"title": "{i18n>pagDeletedEntitiesTitle}",
					"viewId": "deleted-list"
				},
				"customizing": {
					"viewName": "misc.Customizing",
					"title": "{i18n>pagCustomizingTitle}",
					"viewId": "customizing"
				},
				"import": {
					"viewName": "misc.Import",
					"title": "{i18n>pagImportTitle}",
					"viewId": "import"
				},
				"server-settings": {
					"viewName": "misc.ServerSettings",
					"title": "{i18n>pagServerSettingsTitle}",
					"viewId": "server-settings"
				},
				"user-settings": {
					"viewName": "misc.UserSettings",
					"title": "{i18n>pagUserSettingsTitle}",
					"viewId": "user-settings"
				},
				"help": {
					"viewName": "misc.Help",
					"title": {
						"parts": ["i18n>titHelpPageMasterText", "i18n>pagHelpTopicTitle", "view>title"],
						"formatter": "spet.sbwo.web.model.formatter.helpPageTitle"
					},
					"viewId": "help"
				},
				"expertise-create": {
					"viewName": "wizard.ExpertiseCreate",
					"title": "{i18n>pagExpertiseCreateTitle}",
					"viewId": "expertise-create"
				},
				"expertise-list": {
					"viewName": "list.ExpertiseList",
					"title": "{i18n>pagExpertiseListTitle}",
					"viewId": "expertise-list"
				},
				"expertise": {
					"viewName": "entity.Expertise",
					"title": {
						"parts": [
							"view>/loaded", "i18n>pagEntityLoadingTitle", 
							"i18n>pagEntityExpertisePatternTitle", "data>/number"
						],
						"formatter": "spet.sbwo.web.model.formatter.entityPageTitle"
					},
					"viewId": "expertise"
				}
			}
		}
	}
}