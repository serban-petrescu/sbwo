{
	"fields": {
		"expertise": {
			"caseNumber": {
				"regex": "^\\d+[^/]*/(\\d+)[^/]*/(\\d+).*$",
				"severity": "Warning",
				"text": "i18n>txtExpertiseCaseNumberFormatError",
				"captures": [{
					"target": "court/code",
					"force": true
				},{
					"target": "year",
					"force": true,
					"type": "integer"
				}]
			}
		},
